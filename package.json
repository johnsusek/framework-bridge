{
  "name": "framework-bridge",
  "version": "0.0.1",
  "author": "John Susek",
  "license": "MIT",
  "repository": "https://github.com/johnsusek/framework-bridge.git",
  "scripts": {
    "clean": "rm -rf test/testapp/tmp tmp/ .build/ .swiftpm/ node_modules/",
    "test": "./test/bridge.js",
    "testapp:gen": "mkdir -p test/testapp/build && xcodegen -s test/testapp/project.json -p test/testapp/build",
    "testapp:build": "cd test/testapp/build && xcodebuild -allowProvisioningUpdates -derivedDataPath ./build -scheme vk-app-mine && cd ../..",
    "testapp:run": "./test/testapp/build/build/Build/Products/Debug/vk-app-mine.app/Contents/MacOS/vk-app-mine",
    "testapp": "npm run testapp:gen && npm run testapp:build && npm run testapp:run"
  },
  "devDependencies": {
    "concurrent-queue": "^7.0.2"
  }
}